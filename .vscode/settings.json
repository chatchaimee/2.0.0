{
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascript",
    "*.ts": "typescriptreact",
    "*.tsx": "typescriptreact"
  }
}
