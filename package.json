{
  "name": "chatchai-meesuksabai",
  "author": "@chatchaimee",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "check-libs-versions": "ncu -u",
    "export": "next export",
    "deploy": "rm -rf node_modules/.cache && rm -rf out/ && next build && next export && touch out/.nojekyll && gh-pages -d out -m \"Deploy $(git rev-parse HEAD) to GitHub pages [ci skip]\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@types/next": "9.0.0",
    "@types/react-dom": "17.0.6",
    "gh-pages": "1.1.0",
    "next": "11.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.4",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/styled-components": "5.1.10",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-jest": "27.0.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-define": "2.0.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.4",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "typescript": "4.3.4"
  }
}
